{
  "name": "watch-repair-website",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "heroku-postbuild": "webpack --config webpack.prod.js",
    "start": "node src/server/server.js",
    "dev": "start webpack-dev-server --config webpack.dev.js --history-api-fallback",
    "server": "start nodemon src/server/server.js",
    "start-dev-server": "npm-run-all --parallel dev server",
    "pretest": "cross-env NODE_ENV=test sequelize db:create && cross-env NODE_ENV=test npx sequelize db:migrate && cross-env NODE_ENV=test npx sequelize db:seed:all",
    "test": "jest",
    "posttest": "cross-env NODE_ENV=test npx sequelize db:drop",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src ",
    "lint-fix": "eslint ./src --fix"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
