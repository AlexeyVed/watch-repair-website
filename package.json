{
  "name": "watch-repair-website",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "build": "start webpack --mode development",
    "start": "start webpack-dev-server --mode development",
    "server": "start nodemon src/server/server.js",
    "all-start": "npm-run-all --parallel start server",
    "test": "mocha --opts ./config/mocha.opts src/client/**/*.test.js --recursive --silent",
    "test-cov": "nyc -e .jsx mocha --opts ./config/mocha.opts src/**/*.test.js --recursive -R nyan --silent",
    "update-snapshots": "mocha --opts ./config/mocha.opts src/**/*.test.js --require mocha-snapshots --update"
  },
  "author": "",
  "license": "",
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/__snapshots__/**",
      ".nyc_output/**",
      ".nyc_output-tmp/**",
      "config/mocha-setup.js",
      "config/mocha.opts",
      "**/mockData/**"
    ],
    "cache": true,
    "all": false,
    "temp-directory": "./.nyc_output-tmp",
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.6",
    "@material-ui/core": "^4.0.1",
    "@material-ui/pickers": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.27",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^1.0.0",
    "cors": "^2.8.5",
    "css-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "mocha-snapshots": "^4.2.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "sinon": "^7.3.0"
  }
}
