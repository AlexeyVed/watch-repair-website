{
  "name": "watch-repair-website",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "heroku-postbuild": "webpack -p",
    "server": "node src/server/server.js",

    "start-dev": "start webpack-dev-server --mode development --history-api-fallback",
    "server-dev": "start nodemon src/server/server.js",
    "all-start": "npm-run-all --parallel start-dev server-dev",

    "test": "mocha --opts ./config/mocha.opts src/client/**/*.test.js --recursive --silent",
    "test-cov": "nyc -e .jsx mocha --opts ./config/mocha.opts src/**/*.test.js --recursive -R nyan --silent",
    "update-snapshots": "mocha --opts ./config/mocha.opts src/**/*.test.js --require mocha-snapshots --update"
  },
  "author": "",
  "license": "",
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/__snapshots__/**",
      ".nyc_output/**",
      ".nyc_output-tmp/**",
      "config/mocha-setup.js",
      "config/mocha.opts",
      "**/mockData/**"
    ],
    "cache": true,
    "all": false,
    "temp-directory": "./.nyc_output-tmp",
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.2.3",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.13.0",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "cors": "^2.8.5",
    "css-loader": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mocha": "^6.0.2",
    "mocha-snapshots": "^4.2.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "redux-devtools-extension": "^2.13.8",
    "sinon": "^7.3.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.7.1"
  }
}
